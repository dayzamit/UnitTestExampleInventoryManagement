<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leapfrog.inventorymanagementsystem"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.leapfrog.inventorymanagementsystem.MisumiApplication"
        android:allowBackup="true"
        android:icon="@drawable/luncher_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.leapfrog.inventorymanagementsystem.login.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.leapfrog.inventorymanagementsystem.itemdetail.ItemDetailActivity" />
        <activity android:name="com.leapfrog.inventorymanagementsystem.dashboard.DashBoardActivity" />
        <activity android:name="com.leapfrog.inventorymanagementsystem.cart.CartActivity" />
        <activity android:name="com.leapfrog.inventorymanagementsystem.favorites.FavoritesActivity" />
        <activity android:name="com.leapfrog.inventorymanagementsystem.settings.SettingsActivity" />
        <activity android:name="com.leapfrog.inventorymanagementsystem.payment.PaymentActivity" />
    </application>

</manifest>